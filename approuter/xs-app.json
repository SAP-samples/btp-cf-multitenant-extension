{
  "welcomeFile": "ui/index.html",
    "routes": [
     {
      "source": "^/businesspartner/(.*)$",
      "target": "$1",
      "destination": "businesspartner",
      "scope": "$XSAPPNAME.Coordinator"
    },
    {
      "source": "^/admin/(.*)$",
      "target": "$1",
      "destination": "adminsrv"
